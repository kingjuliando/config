! ~/.Xdefaults
! For for the urxvt(erminal emulator)

! FONTS !
URxvt*font: xft:RobotoMono:style=Light:size=14:pixelsize=13:antialias=true,xft:Font\ Awesome:antialias=true:pixelsize=12

! BASIC !
URxvt*intensityStyles: true
URxvt*linespace: 0
URxvt*geometry: 100x30
URxvt*borderLess: false
URxvt*externalBorder: 0
URxvt*internalBorder: 20
URxvt*scrollBar: false
URxvt.letterSpace: 0.5
!URxvt.background: [85]#1f2f3a
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint:   false

! CURSOR !
URxvt*cursorColor: #FF0000
URxvt*cursorColor2: #FEB109
URxvt*cursorBlink: true
URxvt*reverseVideo: false

! I don't know what 90% of this does !
URxvt*insecure: false
URxvt*termName: rxvt-unicode
URxvt*loginShell: true
URxvt*hold: false
URxvt*saveLines: 5000
URxvt*jumpScroll: true
URxvt*skipScroll: true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true
URxvt*mouseWheelScrollPage: true
URxvt*tripleclickwords: true
URxvt*cutchars: `'",;@&*=|?()<>[]{}
URxvt*pastableTabs: false
URxvt*mapAlert: false
URxvt*urgentOnBell: false
URxvt*visualBell: false
URxvt*utmpInhibit: false

!! Perl Extensions !!
URxvt*perl-ext-common: default,matcher,resize-font,selection-to-clipboard,pasta,digital-clock
! Match URL's !
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-launcher: /usr/bin/firefox
URxvt.underlineURLs: True
URxvt.matcher.button: 1 
! Resize Text !
URxvt.keysym.C-Down: resize-font:smaller
URxvt.keysym.C-Up: resize-font:bigger
URxvt.keysym.C-equal: resize-font:reset
URxvt.keysym.C-question: resize-font:show
! Paste the Text !
URxvt.keysym.Control-Shift-V: perl:pasta:paste

